pipeline {
    agent any 

    stages {
        stage('Checkout') {
            steps {
                // Checkout the code from the repository
                checkout scm
            }
        }
        stage('Test') {
            steps {
                // Run your tests here
                sh 'sudo apt install npm'
                sh 'npm test'
            }
        }
        stage('Build') {
            steps {
                // Build your project here
                sh 'npm run build'
            }
        }
    }
}
